{
  "markdown-pdf.styles": [
    ".vscode/markdown-pdf.css"
  ],
  "markdown-pdf.stylesRelativePathFile": true,
  "markdown-pdf.convertOnSave": true,
  "markdown-pdf.type": [
    "pdf",
    "html"
  ],
  "markdown.trace": "verbose",
  "markdown-pdf.displayHeaderFooter": true,
  "markdown-pdf.headerTemplate": "empty",
  "markdown-pdf.footerTemplate": "<footer style=\"font-size: 10px; margin: 0 auto;\">Page <span class='pageNumber'></span> of <span class='totalPages'></span></footer>",
}
